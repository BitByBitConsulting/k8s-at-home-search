{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon -e ts,md,json,html ./server.ts",
    "build": "tsc && npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client  --ssrManifest  --base=/k8s-at-home-search/",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server  --base=/k8s-at-home-search/",
    "generate": "rm -rf dist; tsc && vite build --outDir dist/static --ssrManifest  --base=/k8s-at-home-search/ && npm run build:server && ts-node --project tsconfig.json prerender.ts",
    "serve": "vite preview --outDir dist/static --base=/k8s-at-home-search/"
  },
  "dependencies": {
    "@iconify/react": "^4.0.1",
    "@types/marked": "^4.0.8",
    "dayjs": "^1.11.7",
    "dompurify": "^2.4.3",
    "jsdom": "^21.0.0",
    "marked": "^4.2.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-github-btn": "^1.4.0",
    "react-router-dom": "^6.6.2",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.4"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^2.2.1",
    "@playwright/test": "^1.30.0",
    "@types/dompurify": "^2.4.0",
    "@types/express": "^4.17.16",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
