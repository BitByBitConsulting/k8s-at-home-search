{
  "name": "flux-search",
  "version": "1.0.0",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead, not ios_saf < 13",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vite build --base=/k8s-at-home-search/",
    "serve": "vite preview --base=/k8s-at-home-search/",
    "build-serve": "yarn run build && yarn run serve",
    "it-test": "cypress run",
    "cypress-open": "cypress open --config pageLoadTimeout=30000,baseUrl=http://localhost:3000",
    "ci": "start-server-and-test 'build-serve' http://localhost:4173/k8s-at-home-search/ 'it-test'"
  },
  "license": "MIT",
  "dependencies": {
    "@twind/react": "^0.0.4",
    "better-sqlite3": "^7.5.3",
    "kysely": "^0.19.5",
    "moment": "^2.29.1",
    "observable-hooks": "^4.2.0",
    "react": ">=16.8.0",
    "react-dom": "^18.1.0",
    "react-github-btn": "^1.3.0",
    "rxjs": "^7.5.2",
    "semver": "^7.3.7",
    "sql.js": "^1.6.2",
    "twind": "^0.16.16",
    "wouter": "^2.8.0-alpha.2"
  },
  "devDependencies": {
    "@iconify/react": "^3.1.3",
    "@testing-library/react": "^13.2.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/sql.js": "^1.4.3",
    "@vitejs/plugin-react": "^1.3.2",
    "cypress": "^10.3.1",
    "start-server-and-test": "^1.14.0",
